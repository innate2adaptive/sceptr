{
    "model": {
        "class": "CDRClsBERT",
        "config": {
            "name": "CDRBERT +ACL (cns 0.20, cdrcns 0.01, chaincns 0.20)",
            "num_encoder_layers": 6,
            "d_model": 128,
            "nhead": 8,
            "dim_feedforward": 512
        },
        "pretrain_state_dict_path": "/home/ynagano/tcr_embedder/model_saves/CDRBERT/state_dict.pt"
    },
    "data": {
        "train_path": "/home/ynagano/scp/tcr_embedder/data/tanno/train.csv",
        "valid_path": "/home/ynagano/scp/tcr_embedder/data/tanno/test.csv",
        "tokeniser": {
            "class": "CDRTokeniser",
            "config": {
                "p_drop_aa": 0.2,
                "p_drop_cdr": 0.01,
                "p_drop_chain": 0.2
            }
        },
        "dataset": {
            "config": {
                "censoring_lhs": true,
                "censoring_rhs": true
            }
        },
        "dataloader": {
            "config": {
                "batch_size": 256,
                "num_workers": 4
            }
        }
    },
    "optim": {
        "contrastive_loss": {
            "class": "SimCLoss",
            "config": {
                "temp": 0.05
            }
        },
        "optimiser": {
            "config": {
                "n_warmup_steps": 10000
            }
        }
    },
    "n_epochs": 20
}