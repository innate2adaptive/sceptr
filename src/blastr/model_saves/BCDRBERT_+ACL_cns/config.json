{
    "model": {
        "class": "BCDRClsBERT",
        "config": {
            "name": "BCDRBERT +ACL (cns)",
            "num_encoder_layers": 6,
            "d_model": 128,
            "nhead": 8,
            "dim_feedforward": 512
        },
        "pretrain_state_dict_path": "/home/ynagano/tcr_embedder/model_saves/BCDRBERT/state_dict.pt"
    },
    "data": {
        "train_path": "/home/ynagano/scp/tcr_embedder/data/tanno/train.csv",
        "valid_path": "/home/ynagano/scp/tcr_embedder/data/tanno/test.csv",
        "tokeniser": {
            "class": "BCDRTokeniser",
            "config": {
                "p_drop_aa": 0,
                "p_drop_cdr": 0
            }
        },
        "autocontrastive_noising": true,
        "dataloader_config": {
            "batch_size": 512,
            "num_workers": 4
        }
    },
    "optim": {
        "contrastive_loss": {
            "name": "SimCLoss",
            "config": {
                "temp": 0.05
            }
        },
        "optimiser_config": {
            "n_warmup_steps": 10000
        }
    },
    "n_epochs": 20,
    "gpu": true
}