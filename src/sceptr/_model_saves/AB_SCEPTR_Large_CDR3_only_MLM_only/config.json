{
    "training_delegate": {
        "class": "MlmTrainingDelegate",
        "initargs": {}
    },
    "model": {
        "name": "AB SCEPTR (Large, CDR3 only, MLM only)",
        "path_to_pretrained_state_dict": null,
        "token_embedder": {
            "class": "Cdr3Embedder",
            "initargs": {
                "embedding_dim": 768
            }
        },
        "self_attention_stack": {
            "class": "SelfAttentionStackWithBuiltins",
            "initargs": {
                "num_layers": 12,
                "d_model": 768,
                "nhead": 12,
                "dim_feedforward": 1536
            }
        },
        "mlm_token_prediction_projector": {
            "class": "AminoAcidTokenProjector",
            "initargs": {
                "d_model": 768
            }
        },
        "vector_representation_delegate": {
            "class": "AveragePoolVectorRepresentationDelegate",
            "initargs": {}
        },
        "trainable_model": {
            "class": "MlmTrainableModel",
            "initargs": {}
        }
    },
    "data": {
        "training_data": {
            "dataset": {
                "class": "TcrDataset",
                "initargs": {}
            },
            "dataloader": {
                "class": "SingleDatasetDataLoader",
                "initargs": {
                    "batch_size": 256,
                    "num_workers": 4
                }
            },
            "csv_paths": [
                "tcr_data/preprocessed/tanno/train.csv"
            ]
        },
        "validation_data": {
            "dataset": {
                "class": "TcrDataset",
                "initargs": {}
            },
            "dataloader": {
                "class": "SingleDatasetDataLoader",
                "initargs": {
                    "batch_size": 256,
                    "num_workers": 4
                }
            },
            "csv_paths": [
                "tcr_data/preprocessed/tanno/test.csv"
            ]
        },
        "tokeniser": {
            "class": "Cdr3Tokeniser",
            "initargs": {}
        },
        "batch_collator": {
            "class": "MlmBatchCollator",
            "initargs": {}
        }
    },
    "loss": {
        "cross_entropy_loss": {
            "class": "AdjustedCrossEntropyLoss",
            "initargs": {
                "label_smoothing": 0.1
            }
        }
    },
    "optimiser": {
        "initargs": {
            "n_warmup_steps": 10000,
            "d_model": 768
        }
    },
    "num_epochs": 100
}